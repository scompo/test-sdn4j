<beans:beans xmlns="http://www.springframework.org/schema/batch"
	xmlns:beans="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
           http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd
           http://www.springframework.org/schema/batch
           http://www.springframework.org/schema/batch/spring-batch-2.2.xsd">

	<job id="importJob">
		<flow parent="importJobFlow" id="importJob.importJobFlow" />
	</job>

	<flow id="importJobFlow">
		<step id="importOperazioneOne" next="createMacroOperazioneOne">
			<tasklet ref="emptyPlaceholderTasklet" />
		</step>
		<step id="createMacroOperazioneOne" next="linkMacroOperazioneOne">
			<tasklet ref="emptyPlaceholderTasklet" />
		</step>
		<step id="linkMacroOperazioneOne">
			<tasklet ref="emptyPlaceholderTasklet" />
		</step>
	</flow>

</beans:beans>